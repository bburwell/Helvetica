{
	"manifest_version": 2,
	"name": "Helvetica",
	"version": "1.0",
	"description": "A simple extension that transforms the web into Helvetica.",
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"homepage_url": "http://www.benburwell.com/software/helvetica/",
	"update_url": "http://stat.benburwell.com/updates/chrome/updates.xml",
	"content_scripts": [{
		"css": [ "helvetica.css" ],
		"matches": [ "<all_urls>" ]
	}]
}